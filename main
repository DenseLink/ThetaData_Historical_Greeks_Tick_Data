    "    #\"datacollection5-Copy3.ipynb\", \n",
    "    #\"datacollection5.ipynb\",\n",
    "    \n",
    "    #\"datacollection6-Copy1.ipynb\" \n",
    "]\n",
    "\n",
    "def run_notebook(notebook_name):\n",
    "    os.system(f\"jupyter nbconvert --to notebook --execute {notebook_name}\")\n",
    "\n",
    "threads = []\n",
    "for notebook in notebooks:\n",
    "    thread = threading.Thread(target=run_notebook, args=(notebook,))\n",
    "    thread.start()\n",
    "    threads.append(thread)\n",
    "    time.sleep(30)  # Delay for 30 seconds before starting the next one\n",
    "\n",
    "for thread in threads:\n",
    "    thread.join()  # Wait for all threads to complete\n"
   ]
